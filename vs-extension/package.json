{
  "name": "python-file-executor",
  "displayName": "Python File Executor",
  "description": "Open and execute Python files from a side panel",
  "version": "0.0.2",
  "engines": {
      "vscode": "^1.60.0"
  },
  "categories": [
      "Other"
  ],
  "activationEvents": [
      "onCommand:python-file-executor.openExecutor"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "commands": [
          {
              "command": "python-file-executor.openExecutor",
              "title": "Open Python File Executor"
          }
      ]
  },
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "pretest": "npm run compile && npm run lint",
      "lint": "eslint src --ext ts",
      "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
      "@types/vscode": "^1.60.0",
      "@types/node": "^14.14.37",
      "@typescript-eslint/eslint-plugin": "^4.22.0",
      "@typescript-eslint/parser": "^4.22.0",
      "eslint": "^7.25.0",
      "typescript": "^4.2.4"
  }
}